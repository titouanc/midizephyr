&usart2 {
    status = "okay";
    pinctrl-0 = <&usart2_tx_pd5 &usart2_rx_pd6>;
    pinctrl-names = "default";
    current-speed = <31250>;
};

&timers2 {
    status = "okay";
    st,prescaler = <10000>;

    pwm2: pwm {
        status = "okay";
    };
};

