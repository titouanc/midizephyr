&pwm1 {
    pinctrl-0 = <
        &tim1_ch1_pe9
        &tim1_ch2_pe11
        &tim1_ch3_pe13
        &tim1_ch4_pe14
    >;
    pinctrl-names = "default";
};

&pwm2 {
    pinctrl-0 = <
        &tim2_ch1_pa5
        &tim2_ch4_pa3
    >;
    pinctrl-names = "default";
